{
  "rules": {
    "class-methods-use-this": "off",
    "import/no-extraneous-dependencies": 1
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
},
  "plugins": [
    "prettier"
],
"extends": [
  "airbnb-base",
  "prettier"
],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  }
}